// WAP to count number oof digit in a number
#include "stdio.h"

int main ()
{
    int n,count=0;
    printf("\n Enter Number :");
    scanf("%d",&n);
    while (n!=0)
    {
        count++;
     n=n/10;

    }
    printf("\n Total number of digits are : %d",count);
    
}
